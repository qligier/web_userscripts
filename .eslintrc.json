{
  "root": true,
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "script",
    "ecmaFeatures": {
      "jsx": false,
      "experimentalObjectRestSpread": true
    }
  },
  "env": {
    "browser": true,
    "es2021": true,
    "greasemonkey": true
  },
  "rules": {
    "indent": [
      "error",
      2
    ]
  }
}
